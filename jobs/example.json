{
  "name": "SimpleJob",
  "info": "This job simply queries some values and compares them to a given min and max range",
  "elasticsearch": {
    "host": "localhost",
    "port": "9200",
    "index": "monitoring",
    "type": "rum"
  },
  "query": {
    "filtered": {
      "query": {
        "query_string": {
          "query": "_exists_:renderTime",
          "analyze_wildcard": true
        }
      },
      "filter": {
        "range" : {
          "timestamp" : {
            "gt" : "now-15m"
          }
        }
      }
    }
  },
  "fieldName": "renderTime",
  "min": 0,
  "max": 500,
  "tolerance": 4,
  "reporters": {
    "console": {}
  }
}
