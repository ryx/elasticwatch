{
  "name": "SimpleTest",
  "info": "This config is meant to query some values and define min and max",
  "index": "monitoring",
  "type": "rum",
  "query": {
    "query_string": {
      "query": "_exists_:renderTime",
      "analyze_wildcard": true
    }
  },
  "min": 0,
  "max": 10,
  "tolerance": 14,
  "reporters": {
    "console": {
      "prefix": "Simple test"
    }
  }
}
